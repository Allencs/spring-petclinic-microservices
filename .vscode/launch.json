            "vmArgs": "-Dspring.profiles.active=local -Dspring.cloud.config.enabled=false -Dspring.datasource.url=jdbc:mysql://localhost:3306/petclinic?useSSL=false&allowPublicKeyRetrieval=true -Dspring.datasource.username=root -Dspring.datasource.password=123456 -Dspring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver -Dspring.jpa.database-platform=org.hibernate.dialect.MySQLDialect -Dspring.sql.init.schema-locations=classpath*:db/mysql/schema.sql -Dspring.sql.init.data-locations=classpath*:db/mysql/data.sql",
